	sSheetName = &quot;Terr&quot; + sTerrID + &quot;_Import&quot;
	RenameSheet(sSheetName)
	ProtectSheet()
	CopyToEnd()
	
	UnprotectSheet()
	RUImportToBridge
	sSheetName = &quot;Terr&quot; + sTerrID + &quot;_Import&quot;
	RenameSheet(sSheetName)
	ProtectSheet()
	CopyToEnd()
	
	UnprotectSheet()
